{
  "name": "@nx-tools/analyze",
  "version": "0.0.0-e2e",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "nx-tools-db": "./dist/cli.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "@nx-tools/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@nx/devkit": "^21.6.3",
    "next": "^14.2.35",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sqlite3": "^5.1.7",
    "tslib": "^2.3.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/sqlite3": "^3.1.11"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "name": "@nx-tools/analyze",
    "targets": {
      "package": {
        "dependsOn": ["build", "project-view:build"],
        "options": {
          "cwd": "{workspaceRoot}",
          "command": "cp -r apps/project-view/.next packages/analyze/dist/"
        }
      },
      "start": {
        "executor": "nx:run-commands",
        "dependsOn": [
          "build"
        ],
        "options": {
          "cwd": "{workspaceRoot}",
          "command": "node ./packages/analyze/dist/cli.js"
        }
      }
    }
  }
}
