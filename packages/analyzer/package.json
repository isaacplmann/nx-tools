{
  "name": "@nx-tools/analyzer",
  "version": "0.0.1",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "outputs": [
          "{options.outputPath}"
        ],
        "options": {
          "outputPath": "packages/analyzer/dist",
          "main": "packages/analyzer/src/index.ts",
          "tsConfig": "packages/analyzer/tsconfig.lib.json",
          "rootDir": "packages/analyzer/src",
          "generatePackageJson": false,
          "assets": [
            {
              "input": "./packages/analyzer/src",
              "glob": "**/!(*.ts)",
              "output": "."
            },
            {
              "input": "./packages/analyzer/src",
              "glob": "**/*.d.ts",
              "output": "."
            }
          ]
        }
      }
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  }
}
